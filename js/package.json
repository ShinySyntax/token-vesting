  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bonfida/token-vesting"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
